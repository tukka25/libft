#include "libft.h"

void	*ft_memchr(const void *s, int c, size_t n)
{
	size_t	i;
	char	*f;

	f = (char *)s;
	i = 0;
	while (f[i] != '\0' && i < n)
	{
		if (f[i] == (char)c)
		{
			return (f + i);
		}
		i++;
	}
	return (NULL);
}
