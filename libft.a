#include "libft.h"

void *ft_memmove(void *dst, const void *src, size_t len)
{
	int	i;
	char *ndst;
	char *nsrc;
	
	i = 0;
	ndst = (char *)dst;
	nsrc = (char *)src;

	while(len != 0)
	{
		ndst[i] = nsrc[i];
		i++;
		len--;
	}
	return (ndst);
}
