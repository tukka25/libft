#include "libft.h"

int	ft_atoi(const char *str)
{
	char	sign;
	int		result;
	int		i;

	result = 0;
	i = 0;
	sign = 1;
	if (str[i] == '-')
	{
		sign = -1;
	}
	if (str[i] == '+')
	{
		sign = 1;
		i++;
	}
	while (str[i] >= 48 && str[i] <= 57)
	{
		result = result * 10 + str[i] - '0';
		i++;
	}
	return (result * sign);
}
int main()
{
