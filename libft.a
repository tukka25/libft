#include "libft.h"

void	*ft_memcpy(void *dst, const void *src, size_t n)
{
	char	*ns;
	size_t	i;
	char	*nd;

	ns = (char *)src;
	nd = (char *)dst;
	i = 0;
	if (dst == NULL)
		return (NULL);
	while (n > 0)
	{
		nd[i] = ns[i];
		i++;
		n--;
	}
	return (dst);
}
