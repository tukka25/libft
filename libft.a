#include "libft.h"

char	*ft_strdup(const char *s1)
{
	int		i;
	char	*s;

	i = 0;
	s = (char *)malloc(sizeof(char));
	while (*s1 != '\0')
	{
		s[i] = *s1;
		i++;
		s1++;
	}
	return (s);
}
