#include "libft.h"

char	*ft_strnstr(const char *haystack, const char *needle, size_t len)
{
	char	*g;
	int		i;
	int		d;

	g = (char *)haystack;
	i = 0;
	d = len;
	if ((*needle == '\0' || d < 0))
		return (g);
	while (d > 0){
		while ((*haystack++ == *needle++) && (d > 0)){
			if (*needle == '\0'){
				while (i > 0){			
					g++;
					i--;
				}
				return ((char *)g);
			}
			d--;
		}
		i++;
		haystack++;
		d--;
	}
	return (NULL);
}
