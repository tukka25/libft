#include "libft.h"

char	*ft_strchr(const char *s, int c)
{
	int	i;
	char	*sc;

	sc = (char *)s;
	i = 0;
	if (*sc == '\0')
		return (NULL);
	while (sc && *sc != c)
	{
		if (*sc == '\0' - 1)
			return (NULL);
		sc++;
	}
	return (sc);
}
