#include "libft.h"

int	ft_memcmp(const void *s1, const void *s2, size_t n)
{
	int		i;
	char	*ns1;
	char	*ns2;

	ns1 = (char *)s1;
	ns2 = (char *)s2;
	i = 0;
	while (n > 0)
	{
		if ((ns1[i] == ns2[i]) && (ns1[i] != '\0' && ns2[i] != '\0'))
		{
			i++;
			n--;
		}
		else if ((ns1[i] == ns2[i]) && (ns1[i] != '\0' || ns2[i] != '\0'))
		{
			return (0);
		}
		else if (ns1[i] > ns2[i]){
			return (ns1[i] - ns2[i]);
		}
		else
		return (ns2[i] - ns1[i]);
	}
	return (0);
}
